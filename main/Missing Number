# missing number (leetcode 268)
class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        l=len(nums)
        return int(l*(l+1)/2-sum(nums))
--------------------------------------------------------------------
Input nums =[3,0,1]
Output 2
Input nums =[0,1]
Output 2
Input nums =[9,6,4,2,3,5,7,0,1]
Output 8
